end, 250, 50)
local label = Playersection:Label("Other")

local isNoClip = false

local toggle
toggle = Playersection:Toggle("NoClip (Real Buggy)", function(bool)
    isNoClip = bool

    if isNoClip then
        for _, model in ipairs(game.Workspace:GetDescendants()) do
            if model:IsA("Model") and model.Name == "Model" then
                local hitBox = model:FindFirstChild("Hitbox")
                if hitBox then
                    for _, part in ipairs(model:GetDescendants()) do
                        if part:IsA("BasePart") then
                            part.Transparency = 0.6
                            part.CanCollide = false
                        end
                    end
                end
            end
        end
    else
        for _, model in ipairs(game.Workspace:GetDescendants()) do
            if model:IsA("Model") and model.Name == "Model" then
                local hitBox = model:FindFirstChild("Hitbox")
                if hitBox then
                    for _, part in ipairs(model:GetDescendants()) do
                        if part:IsA("BasePart") then
                            part.Transparency = 0
                            part.CanCollide = true
                        end
                    end
                end
            end
        end
    end
end)




