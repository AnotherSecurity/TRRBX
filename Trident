-- Settings Section --
local title = Settingssection:Title("Game Settings")
local isFullBright = false
local toggle = Settingssection:Toggle("Full Bright", function(bool)
    isFullBright = bool
    while isFullBright do
        wait()
        game.Lighting.Brightness = 2
        game.Lighting.Ambient = Color3.new(255/255, 255/255, 255/255)
        game.Lighting.GlobalShadows = false
        game.Lighting.FogStart = 999999999999999999
    end
end)
