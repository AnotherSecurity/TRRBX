local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Main",
   LoadingTitle = "Security.lua - Trident",
   LoadingSubtitle = "by unearnedtux on discord",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = true,
      FileName = "Security.lua Hub"
   },
   Discord = {
      Enabled = true,
      Invite = "gTTSwadpey",
      RememberJoins = false
   },
   KeySystem = true,
   KeySettings = {
      Title = "Secuirty.lua Entry",
      Subtitle = "Key System",
      Note = "Join the discrd for the key",
      FileName = "Key",
      SaveKey = false,
      GrabKeyFromSite = true,
      Key = {"https://pastebin.com/raw/DKx97GgJ"}
   }
})

local MainTab = Window:CreateTab("Main")
local MainSection = MainTab:CreateSection("Main Page")

local MaliciousTab = Window:CreateTab("Malicious")
local MaliciousSection = MaliciousTab:CreateSection("Malicious Page")

local SettingsTab = Window:CreateTab("Settings")
local SettingsSection = SettingsTab:CreateSection("Settings Page")

Rayfield:Notify({
   Title = "Executed",
   Content = "You have successfully loaded the script.",
   Duration = 6.5,
   Image = 4483362458,
   Actions = {
      Ignore = {
         Name = "W",
         Callback = function()
      end
   },
},
})


