local isNoClip = false
local recheckingInterval = 5
local partsToCheck = {"Hitbox", "Enclosure", "Wall"}

local originalProperties = {}

local function updateParts()
    if isNoClip then
        for _, model in ipairs(game.Workspace:GetDescendants()) do
            if model:IsA("Model") and model.Name == "Model" then
                for _, part in ipairs(model:GetDescendants()) do
                    if part:IsA("BasePart") and table.find(partsToCheck, part.Name) then
                        if not originalProperties[part] then
                            originalProperties[part] = {
                                Transparency = part.Transparency,
                                CanCollide = part.CanCollide
                            }
                        end

                        part.Transparency = 0.6
                        part.CanCollide = false
                    end
                end
            end
        end
    else
        for part, properties in pairs(originalProperties) do
            part.Transparency = properties.Transparency
            part.CanCollide = properties.CanCollide
            originalProperties[part] = nil
        end
    end
end

local recheckingEnabled = false

local toggle
toggle = Visualssection:Toggle("NoClip (Real Buggy)", function(bool)
    isNoClip = bool
    recheckingEnabled = bool

    if recheckingEnabled then
        while recheckingEnabled do
            updateParts()
            wait(recheckingInterval)
        end
    else
        updateParts()
    end
end)

local toggle = Visualssection:Toggle("Chat", function(bool)
    if bool == true then
        game:GetService("Players").LocalPlayer.PlayerGui.GameUI.Chat.Visible = false
    else bool = false do
        game:GetService("Players").LocalPlayer.PlayerGui.GameUI.Chat.Visible = true
    end
    end
end)

